{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "source": "$HOME/code/imgrepo/pix/stars1.jpg",
    "height": 17,
    // "width": 40,
    "padding": {
      "left": 3,
      "top": 3,
      "right": 3,
    },
  },
  "modules": [
    "break",
    {
      "type": "custom",
      "format": "\u001b[35m  ✴  C V B E R S C Λ P E",
    },
    {
      "type": "custom",
      "format": "\u001b[36m┌──────────────────────────────────────────┐",
    },
    {
      "type": "os",
      "key": "   OS",
      "keyColor": "red",
      "format": "{2}",
    },
    {
      "type": "kernel",
      "key": "   Kernel",
      "keyColor": "red",
      "format": "{0} {2}",
    },
    {
      "type": "disk",
      "key": "  󰙅 File system",
      "keyColor": "yellow",
      "format": "{9}",
    },
    {
      "type": "initsystem",
      "key": "  󰩀 Init system",
      "keyColor": "yellow",
      "format": "{0}",
    },
    {
      "type": "shell",
      "key": "   Shell",
      "keyColor": "yellow",
      "format": "{0}",
    },
    {
      "type": "wm",
      "key": "   WM",
      "keyColor": "green",
      "format": "{pretty-name}",
    },
    {
      "type": "terminal",
      "key": "   Terminal",
      "keyColor": "green",
      "format": "{0}",
    },
    {
      "type": "editor",
      "key": "  󰯂 Editor",
      "keyColor": "green",
      "format": "{2}",
    },
    {
      "type": "packages",
      "key": "   Packages",
      "keyColor": "blue",
    },
    {
      "type": "uptime",
      "key": "   Uptime",
      "keyColor": "blue",
    },
    {
      "type": "custom",
      "format": "\u001b[36m└──────────────────────────────────────────┘",
    },
    {
      "type": "custom",
      "format": "\u001b[35m  󰪫 System\u001b[0m:",
    },
    {
      "type": "custom",
      "format": "\u001b[36m┌──────────────────────────────────────────┐",
    },
    {
      "type": "cpu",
      "key": "   CPU",
      "keyColor": "white",
      "format": "{}",
    },
    {
      "type": "gpu",
      "key": "   GPU",
      "keyColor": "white",
      "format": "{2}",
    },
    {
      "type": "memory",
      "key": "   Ram",
      "keyColor": "yellow",
    },
    {
      "type": "disk",
      "key": "   Disk",
      "keyColor": "yellow",
      "format": "{1} / {2} ({3})",
    },
    {
      "type": "custom",
      "format": "\u001b[36m└──────────────────────────────────────────┘",
    },
    {
      "type": "battery",
      "key": "   Battery",
      "keyColor": "yellow",
    },
    {
      "type": "datetime",
      "key": "   Time",
      "keyColor": "yellow",
      "format": "{14}:{18} - {11}.{4}.{1} ",
    },
    {
      "type": "colors",
      "paddingLeft": 2,
      "symbol": "circle",
    },
  ],
}
